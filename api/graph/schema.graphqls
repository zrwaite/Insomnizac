type Language {
  name: String!
  image: String!
}

type Project {
  id: ID!
  name: String!
  slug: String!
  description: String!
  githubName: String!
  languages: [Language!]!
  devpostLink: String
  projectLink: String
  createdAt: String!
  updatedAt: String!
  image: String
  featured: Boolean!
}

type Query {
  projects: [Project!]!
  project(slug: String!): Project!
}

type Mutation {
  contact(message: String!): Boolean!
}
