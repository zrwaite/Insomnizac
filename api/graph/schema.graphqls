# GraphQL schema example
type Project {
  id: ID!
  name: String!
  slug: String!
  description: String
  githubName: String!
  devpostLink: String
  projectLink: String
  createdAt: String!
  updatedAt: String!
}

type Query {
  projects: [Project!]!
}

type Mutation {
  contact(message: String!): Boolean!
}
